# TECO configuration â€” copy to teco.toml and fill in your values.
# Settings here are overridden by environment variables (see below).
#
# Env vars:  TECO_MODEL, TECO_API_KEY, TECO_BASE_URL, OPENAI_API_KEY, TECO_VERBOSITY

[llm]
model    = "MiniMaxAI/MiniMax-M2.5"                 # any model ID supported by your provider
api_key  = ""                                       # leave empty to use OPENAI_API_KEY from env
base_url = "https://api.deepinfra.com/v1/openai"    # override for non-OpenAI providers (vLLM, Ollama, etc.)

[output]
verbosity = 1   # 0=quiet  1=normal  2=verbose (diffs, tables)  3=debug (full profiling dumps)
